#include<stdio.h>

int prime(int n)
{
	if (n==0 || n==1)
	{
		return false;
	}
	else 
	{
		for(int i=2;i<n;i++)
		{
			if(n%i==0)
			{
				return false;
			}
		}
	}
	return true;
}
int main()
{
	int num;
	printf("sai suryha 192210685\n");
	printf("Enter the number:");
	scanf("%d",&num);
	for(int i=0;i<=num;i++)
	{
		if(prime(i))
		{
			printf("%d ",i);
		}
	}
	return 0;
}
