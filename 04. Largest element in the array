#include<stdio.h>
int main()
{
	int n;
	printf("sai suryha (192210685)\n");
	printf("Enter the no of element in an array:");
	scanf("%d",&n);
	int arr[n];
	printf("Enter the array element:");
	for (int i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	int l=arr[0];
	for(int i=0;i<n;i++){
		if(arr[i]>arr[0])
		{
			l=arr[i];
		}
	}
	printf("Largest element in the array=%d",l);
	return 0;
	
}
