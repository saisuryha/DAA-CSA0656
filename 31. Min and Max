#include <stdio.h>

int main() {
    int n, i;
   printf("sai suryha 192210685\n");

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("The number of elements must be positive.\n");
        return 1;
    }

    int arr[n];
    

    printf("Enter the elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int min = arr[0];
    int max = arr[0];

    for (i = 1; i < n; i++) {
        if (arr[i] < min) {
            min = arr[i];
        }
        if (arr[i] > max) {
            max = arr[i];
        }
    }

    printf("Minimum value: %d\n", min);
    printf("Maximum value: %d\n", max);

    printf("Sequence from min to max: ");
    for (i = min; i <= max; i++) {
        printf("%d ", i);
    }
    printf("\n");

    return 0;
}
