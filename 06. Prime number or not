#include<stdio.h>
int main()
{
	int num,flag=0;
	printf("sai suryha (192210685)\n");
	printf("Enter the positive number:");
	scanf("%d",&num);
	if(num==0 || num==1)
	{
		flag=1;
	}
	for(int i=2;i<=num/2;++i)
	{
		if(num%i==0)
		{
			flag=1;
			break;
		}
	}
	if(flag==0)
	{
		printf("Given number is a Prime Number");
	}
	else
	{
		printf("Given number is not a Prime Number");
	}
	return 0;
}
